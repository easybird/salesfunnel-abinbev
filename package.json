{
  "name": "salesfunnel-abinbev",
  "version": "0.0.1",
  "engines": {
    "node": "4.0.0"
  },
  "private": true,
  "scripts": {
    "auto-start": "forever -w ./bin/www",
    "check-coverage": "istanbul check-coverage --statements 80 --branches 65 --functions 70 --lines 80",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "node ./bin/www",
    "start-debug": "DEBUG=salesfunnel-abinbev:* npm start",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "test": "NODE_ENV=test mocha -w --compilers js:babel-register --recursive",
    "test-single": "NODE_ENV=test istanbul cover -x *.test.js _mocha -- --compilers js:babel-register --recursive"
  },
  "dependencies": {
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.5.2",
    "bluebird": "3.3.3",
    "body-parser": "~1.13.2",
    "connect-mongo": "1.1.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "1.13.0",
    "jade": "~1.11.0",
    "lodash": "4.5.1",
    "migrate": "0.2.2",
    "mongodb": "2.1.7",
    "mongoose": "4.4.5",
    "morgan": "~1.6.1",
    "node-mongoose-fixtures": "0.2.4",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "serve-favicon": "~2.3.0"
  },
  "keywords": [
    "easybird",
    "ab inbev",
    "salesfunnel"
  ],
  "license": "MIT",
  "author": "easybird.be <jonathan@easybird.be> (http://easybird.be/)",
  "bugs": {
    "url": "https://github.com/easybird/salesfunnel-abinbev/issues"
  },
  "homepage": "https://github.com/easybird/salesfunnel-abinbev#readme",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "ghooks": "1.0.3",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "2.4.5",
    "supertest": "1.2.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test-single && npm run check-coverage"
    }
  }
}
